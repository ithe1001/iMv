<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 尺寸定义 -->
    <size android:height="18dp" />

    <!-- 圆角设置 -->
    <corners android:radius="2dp" />

    <!-- 填充颜色 -->
    <solid android:color="#2f00" />

    <!-- 边框设置 -->
    <stroke
        android:width="1dp"
        android:color="#4F00" />
</shape>